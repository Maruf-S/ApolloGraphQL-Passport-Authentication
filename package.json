{
  "name": "kushna_backend",
  "version": "1.0.0",
  "description": "Test Run 1.0",
  "scripts": {
    "build": "tsc",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "dev": "nodemon server.ts",
    "dev:migrate": "npm run migrations:run && npm run dev",
    "dist": "node dist/server.js",
    "dist:migrate": "npm run migrations:run && node dist/server.js",
    "migrations:generate": "prisma migrate dev --name",
    "migrations:run": "prisma migrate deploy",
    "seed": "tsc && node src/db/seed.js",
    "generate": "graphql-codegen"
  },
  "engines": {
    "node": ">=8.0"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./src/db/seed.ts",
    "schema": "src/db/prisma/schema.prisma"
  },
  "license": "MIT",
  "devDependencies": {
    "@graphql-codegen/add": "^4.0.1",
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/graphql-modules-preset": "^3.1.1",
    "@graphql-codegen/typescript": "^3.0.2",
    "@graphql-codegen/typescript-resolvers": "^3.1.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/helmet": "4.0.0",
    "@types/joi": "^17.2.2",
    "@types/winston": "^2.4.4",
    "nodemon": "^2.0.22",
    "prisma": "^4.11.0",
    "ts-node-dev": "^2.0.0",
    "tslint": "^6.1.2",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@apollo/server": "^4.5.0",
    "@prisma-korea/graphql-schema-generator": "^0.3.0",
    "@prisma/client": "^4.11.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^18.15.3",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/uuid": "^8.3.4",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dayjs": "^1.10.7",
    "dotenv": "^8.6.0",
    "express": "^4.18.2",
    "express-rate-limit": "^5.5.1",
    "graphql": "^16.6.0",
    "graphql-modules": "^2.1.2",
    "helmet": "^4.5.0",
    "husky": "^6.0.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.6",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "hooks": {
    "pre-commit": "npm run lint"
  }
}
