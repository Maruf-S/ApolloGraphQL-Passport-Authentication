{
  "name": "kushna_backend",
  "version": "1.0.0",
  "description": "Test Run 1.0",
  "scripts": {
    "start": "export PORT=80 || set PORT=80 && npm run transpile && node dist/server.js",
    "dev": "concurrently \"npm:watch\" \"npm:serve\" \"npm:copy-graphql\"",
    "copy-graphql": "rimraf  dist/src/graphql/typedefs && cp -Rf src/graphql/typedefs dist/src/graphql/typedefs",
    "serve": "node dist/server.js",
    "watch": "tsc -w",
    "transpile": "tsc",
    "seed": "npm run transpile && node src/db/seed.js",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "forever": "export PORT=80 || set PORT=80 && npm run transpile && forever start dist/server.js"
  },
  "engines": {
    "node": ">=8.0"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./src/db/seed.ts",
    "schema": "src/db/prisma/schema.prisma"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/helmet": "4.0.0",
    "@types/joi": "^17.2.2",
    "@types/winston": "^2.4.4",
    "concurrently": "^6.0.2",
    "nodemon": "^2.0.7",
    "prisma": "^4.11.0",
    "ts-node-dev": "^2.0.0",
    "tslint": "^6.1.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@apollo/server": "^4.5.0",
    "@prisma/client": "^4.11.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^18.15.3",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/uuid": "^8.3.4",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dayjs": "^1.10.7",
    "dotenv": "^8.6.0",
    "express": "^4.18.2",
    "express-rate-limit": "^5.5.1",
    "graphql": "^16.6.0",
    "helmet": "^4.5.0",
    "husky": "^6.0.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.6",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "hooks": {
    "pre-commit": "npm run lint"
  }
}
